#include <iostream>
#include <string>
#include <vector>
using std::cout;
using std::cin;
using std::endl;
using std::string;
using std::vector;
inline bool isShorter(const string &s1, const string &s2){
    return s1.size()<s2.size();
}
int main(){
    string s1="hi", s2="hi!";
    if(isShorter(s1,s2)){
        cout<< "true!"<<endl;
    }
    else{
        cout<< "false!" << endl;
    }
    return 0;
}


/*
output:
true!
