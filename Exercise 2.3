#include <iostream>
int main(){
    std::cout<< "short limits are:"<<std::endl;
    std::cout<< std::numeric_limits<short>::min()<<std::endl;
    std::cout<< std::numeric_limits<short>::max() << std::endl;

    std::cout<< "unsigned short limits are:"<<std::endl;
    std::cout<< std::numeric_limits<u_short>::min()<<std::endl;
    std::cout<< std::numeric_limits<u_short>::max() << std::endl;
    
}
/*
short limits are:
-32768
32767
unsigned short limits are:
0
65535
*/
