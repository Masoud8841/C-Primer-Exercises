#include <iostream>
#include <string>
#include <vector>
using std::cout;
using std::cin;
using std::endl;
using std::string;
using std::vector;
int factorial(int);
int main(){
    cout<<"enter a small non-negative integer:"<<endl;
    int n;
    cin >> n;
    cout<<n<<"!= "<<factorial(n)<<endl;
    return 0;
}
int factorial(int n){
    int ret_val=1;
    while(n>1)
        ret_val*=n--;
    return ret_val;
}
