#include <iostream>
#include <vector>
using std::cout;
using std::endl;
using std::vector;
int main(){
    vector<int> ivec(10,3);
    for(vector<int>::iterator iter=ivec.begin();iter!=ivec.end();++iter){
        cout<<*iter <<" ";
        *iter=2*(*iter);
    }
    cout<<endl;
    for(vector<int>::iterator iter=ivec.begin();iter!=ivec.end();++iter){
        cout<<*iter <<" ";
    }
    cout<< endl;
    return 0;
}
