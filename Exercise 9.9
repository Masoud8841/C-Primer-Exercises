#include <iostream>
#include <string>
#include <vector>
#include <list>
#include <deque>
using std::cout;
using std::cin;
using std::endl;
using std::string;
using std::vector;
using std::list;
using std::deque;

int main(){
    list<int> list1;
    list1.push_back(0);list1.push_back(1);list1.push_back(2);list1.push_back(3);

    list<int>::reverse_iterator iter=list1.rbegin();
    cout<<"list1 elements in reverse order are:"<<endl;
    while(iter!=list1.rend()){
        cout<<*iter++<<" ";
    }
    cout<<endl;
    return 0;
}

