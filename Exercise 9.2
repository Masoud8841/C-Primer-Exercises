#include <iostream>
#include <string>
#include <vector>
#include <list>
using std::cout;
using std::cin;
using std::endl;
using std::string;
using std::vector;
using std::list;
 void print_ivec(vector<int> &ivec){
    static int i=1;
    cout<<"ivec"<<i<<":"<<endl;
    for(vector<int>::iterator it=ivec.begin(); it != ivec.end();++it){
        cout<<*it<<" ";
    }
    cout<<"\n"<<endl;
    ++i;
    return;
 }

int main(){
    int ia[]={0,1,1,2,3,5,8};
    vector<int> ivec; ivec.push_back(0); ivec.push_back(1); ivec.push_back(1); ivec.push_back(2);
    ivec.push_back(3); ivec.push_back(5); ivec.push_back(8);

    vector<int> ivec1(ivec); // copy another vector
    vector<int> ivec2(ia,ia+7); // copy using pointers/iterators from vectors/arrays
    vector<int> ivec3(7); // 7 elements initialized with zero
    vector<int> ivec4(7,1); // 7 elements with initial value 1 for each

    print_ivec(ivec1);
    print_ivec(ivec2);
    print_ivec(ivec3);
    print_ivec(ivec4);

    return 0;
}

