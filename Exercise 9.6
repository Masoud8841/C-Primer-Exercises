using std::string;
using std::vector;
using std::list;
using std::deque;

class Foo{
    public:
    Foo(int a){
        cnt=a;
    };
    void print(){
        static int i=0;
        cout<< i<< ": Foo contains: "<<cnt << endl;
        ++i;
        return;
    };
    private:
    int cnt;
};

int main(){
    
    list<Foo> Foo_list(10,5);
    for(list<Foo>::iterator it=Foo_list.begin(); it!=Foo_list.end();++it){
        it->print();
    }

    return 0;
}

