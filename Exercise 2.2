#include <iostream>
int main(){
    std::cout<< "int limits are:"<<std::endl;
    std::cout<< std::numeric_limits<int>::min()<<std::endl;
    std::cout<< std::numeric_limits<int>::max() << std::endl;

    std::cout<< "unsigned int limits are:"<<std::endl;
    std::cout<< std::numeric_limits<u_int>::min()<<std::endl;
    std::cout<< std::numeric_limits<u_int>::max() << std::endl;
    
}
